
                <!--Start Content-->
                <div id="content">
                    <!--Start Main Title-->
                    <div id="main-title-content">
                        <img src="assets/properties/slider/fslider/p1.jpg" id="bg" alt="main-title"/>
                        <div class="container">
                            <div class="panel">
                                <h1>Upload Items</h1>    
                                <p>Sub Title Here</p>    
                            </div>
                        </div>
                    </div> <!--End Main Title-->
                    <!--    News Carousel-->
                    <div class="news-carousel-wrapper">
                        <dl id="news-carousel" class="news-carousel-style">
                            <dt><a href="#">News Link Here</a></dt>
                             <dt>Properties list</dt>
                        <dd><% @properties.each do |p| %> </dd>
                        <dt><a href="#">Owner Name</a></dt>
                        <dd><%= p.name %></dd>
                        <dt><a href="#">Address</a></dt>
                        <dd><%= p.address %></dd>
                         <% end %>
                        </dl>
                    </div>
                    <!--    End News Carousel-->
                    <div class="container">
                        <div class="panel">
                            <div class="box-line">
                                <h3 class="title-heading">Please complete the form</h3>
                            </div>
                            <!-- Start List Step Upload-->
                            <ul class="unstyled proper-step">
                                <li><a href="/upload_step1" class="active"><span>1</span></a></li>
                                <li><a href="/upload_step2" ><span>2</span></a></li>
                                <li><a href="/upload_step3"><span>3</span></a></li>
                            </ul>
                            <div class="clearfix"></div>
                            <!-- Start Form Step -->
                            <form class="proper-form-step">
                                <div class="proper-form-step-area">
                                    <h4>Personal Information</h4>
                                    <div class="row-fluid">
                                        <div class="span4">
                                            <label>Name</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                        <div class="span4">
                                            <label>Email</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                        <div class="span4">
                                            <label>Telephone</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="span4">
                                            <label>State</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                        <div class="span4">
                                            <label>City</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                        <div class="span4">
                                            <label>Address</label>
                                            <input type="text" class="input-block-level" required=""/>
                                        </div>
                                    </div>
                                </div>

                                <button  class="btn btn-proper btn-large">Next Step <i class="icon-arrow-right"></i></button>
                            </form><!-- End Form Step -->
                        </div>

                    </div>
                </div><!--EndContent-->
